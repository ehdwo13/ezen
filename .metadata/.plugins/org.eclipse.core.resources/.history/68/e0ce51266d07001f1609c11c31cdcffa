<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="true">

<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
	<encoder>
		<pattern>%d %5p - %msg%n</pattern>
    </encoder>
</appender>

	<logger name="" level="" appender-ref="/"/>

<root level="info">
	<appender-ref ref="STDOUT" />
</root>
</configuration>